    <script defer>
        document.addEventListener('DOMContentLoaded', function () {
            const popup = document.getElementById('cookie-consent-popup');
            const overlay = document.getElementById('cookie-consent-overlay');
            const acceptBtn = document.getElementById('accept-cookies-btn');
            const continueLink = document.querySelector('.continue-link');

            // Vérifiez si l'utilisateur a déjà accepté ou refusé les cookies
            if (!localStorage.getItem('cookiesAccepted') && !localStorage.getItem('cookiesDeclined')) {
                popup.classList.add('visible');
                overlay.classList.add('visible');
            }

            acceptBtn.addEventListener('click', function () {
                localStorage.setItem('cookiesAccepted', 'true');
                popup.classList.remove('visible');
                overlay.classList.remove('visible');

                // Initialisez Google Analytics ici après acceptation des cookies
                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

                ga('create', 'UA-XXXXX-Y', 'auto');
                ga('send', 'pageview');
            });

            continueLink.addEventListener('click', function (e) {
                e.preventDefault();
                localStorage.setItem('cookiesDeclined', 'true');
                popup.classList.remove('visible');
                overlay.classList.remove('visible');
            });
        });
    </script>
