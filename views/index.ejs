    <div id="cookie-consent-popup" class="cookie-consent-popup">
        <h4>Gestion des Cookies</h4>
        <p>Nous utilisons des cookies pour améliorer votre expérience sur notre site. Vous pouvez accepter ou refuser les cookies non essentiels.</p>
        <button id="accept-cookies-btn" class="btn-accept">Accepter</button>
        <a href="#" class="continue-link">Continuer sans accepter</a>
    </div>

    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const popup = document.getElementById('cookie-consent-popup');
            const acceptBtn = document.getElementById('accept-cookies-btn');
            const continueLink = document.querySelector('.continue-link');

            if (!localStorage.getItem('cookiesAccepted') && !localStorage.getItem('cookiesDeclined')) {
                popup.classList.add('visible');
            }

            acceptBtn.addEventListener('click', function () {
                localStorage.setItem('cookiesAccepted', 'true');
                popup.classList.remove('visible');

                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

                ga('create', 'UA-XXXXX-Y', 'auto');
                ga('send', 'pageview');
            });

            continueLink.addEventListener('click', function (e) {
                e.preventDefault();
                localStorage.setItem('cookiesDeclined', 'true');
                popup.classList.remove('visible');
            });
        });
    </script>

